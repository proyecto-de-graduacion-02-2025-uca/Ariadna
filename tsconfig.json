{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "types": ["node"], 
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "rootDir": "src",
    "outDir": "dist",
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
